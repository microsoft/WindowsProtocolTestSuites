		    </ClaimType>
            <ClaimType>
			    <DisplayName>Company</DisplayName>
                <Values>
                <Value>Contoso</Value>
                <Value>Kerb</Value>
                </Values>
		    </ClaimType>
            <ClaimType>
			    <DisplayName>CountryCode</DisplayName>
                <Values>
                <Value>840</Value>
                <Value>392</Value>
                <Value>156</Value>
                </Values>			   
		    </ClaimType>
        </ClaimTypes>		
        <ResourceProperties>
            <ResourceProperty>
		  	    <DisplayName>Company_MS</DisplayName>
		  	    <ValueType>MS-DS-SinglevaluedChoice</ValueType>
                <Values>
                <Value>contoso</Value>
                <Value>kerb</Value>                
                </Values>		  	    
		    </ResourceProperty>	
		    <ResourceProperty>
		  	    <DisplayName>SecurityLevel</DisplayName>
		  	    <ValueType>MS-DS-SinglevaluedChoice</ValueType>
                <Values>
                <Value>HBI</Value>
                <Value>MBI</Value>
                <Value>LBI</Value>
                </Values>		  	    
		    </ResourceProperty>
		    <ResourceProperty>
		  	    <DisplayName>Department</DisplayName>
		  	    <ValueType>MS-DS-SinglevaluedChoice</ValueType>
                <Values>
                <Value>Payroll</Value>
                <Value>IT</Value>
                </Values>
		  	    
		    </ResourceProperty>
            <ResourceProperty>
		  	    <DisplayName>CountryCode</DisplayName>
		  	    <ValueType>MS-DS-SinglevaluedChoice</ValueType>		  	    
                <Values>
                <Value>156</Value>
                <Value>840</Value>
                <Value>392</Value>
                </Values>            
		    </ResourceProperty>
        </ResourceProperties>
        <Rules>		
             <Rule>
		  	    <Name>PET-AccessRule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/Company == "contoso"))</RuleItem>
		    </Rule>    
            <Rule>
		  	    <Name>CountryCodeEquals156Rule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/CountryCode == 156))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeNotEquals156Rule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/CountryCode != 156))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeLessThan392Rule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/CountryCode Less than 392))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeLessThanOrEquals392Rule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/CountryCode Less than or equals 392))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeGreaterThan392Rule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/CountryCode Greater than 392))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeGreaterThanOrEquals392Rule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/CountryCode Greater than or equals 392))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeAnyOf156Or840Rule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/CountryCode Any_of {156,840}))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeNotAnyOf156Or840Rule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;(@USER.ad://ext/CountryCode Not_Any_of {156,840}))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeEquals156AndITDepartmentRule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;((@USER.ad://ext/CountryCode == 156) And (@USER.ad://ext/Department == "IT")))</RuleItem>
		    </Rule>
            <Rule>
		  	    <Name>CountryCodeEquals156OrITDepartmentRule</Name>
                <ResourceCondition></ResourceCondition>
                <RuleItem>(XA;;FA;;;AU;((@USER.ad://ext/CountryCode == 156) || (@USER.ad://ext/Department == "IT")))</RuleItem>
		    </Rule>
        </Rules>
        <Policies>            
            <Policy>
		  	    <Name>PET-AccessPolicy</Name>
                <Rules>
                    <Rule>PET-AccessRule</Rule>
                </Rules>
		    </Policy>
            <Policy>
		  	    <Name>CountryCodeEquals156Policy</Name>
                <Rules>
                    <Rule>CountryCodeEquals156Rule</Rule>
                </Rules>
		    </Policy>

            <Policy>
		  	    <Name>CountryCodeNotEquals156Policy</Name>
