                </Rules>
		    </Policy>
            <Policy>
		  	    <Name>CountryCodeEquals156OrITDepartmentPolicy</Name>
                <Rules>
                    <Rule>CountryCodeEquals156OrITDepartmentRule</Rule>
                </Rules>
		    </Policy>
        </Policies>	
    </LocalRealm>

