@model IEnumerable<ICodecParam>

@{
    // if an EntropyAlgorithm instance is not found, an exception will be thrown 
    var reduceExtrapolate = (UseReduceExtrapolate)Model.First(param => param is UseReduceExtrapolate);
}

<div style="display: inline-block; margin-right: 12px;">
    <div class="input-group">
        <label>ReduceExtrapolate</label>
        <input class="use-reduce-extrapolate" type="checkbox" @reduceExtrapolate.Enabled ? @Html.Raw("checked=\"checked\"") : @Html.Raw("") />
    </div>
</div>
