<Page x:Class="Microsoft.Protocols.TestManager.UI.TestSuiteWindow"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:System="clr-namespace:System;assembly=mscorlib"
      xmlns:ei="http://schemas.microsoft.com/expression/2010/interactions" xmlns:ee="http://schemas.microsoft.com/expression/2010/effects"
      mc:Ignorable="d" 
      d:DesignHeight="500" d:DesignWidth="700"
        Foreground="{DynamicResource {x:Static SystemColors.ControlTextBrushKey}}"
      Focusable="False"
	Title="TestSuiteWindow">
    <Grid  x:Name="Main" Margin="0,0,0,0">
        <Border BorderThickness="1">
            <ScrollViewer Name="ScrollContent" Margin="0,0,0,0" AutomationProperties.Name="Test suites" PreviewMouseWheel="ScrollViewer_PreviewMouseWheel" BorderThickness="1" Focusable="False">
            </ScrollViewer>
        </Border>
    </Grid>
</Page>
